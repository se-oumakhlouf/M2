<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean class="fr.uge.jee.onlineshop.ReturnInsurance">
        <constructor-arg value="true"/>
    </bean>

    <bean class="fr.uge.jee.onlineshop.ThiefInsurance" />

    <bean class="fr.uge.jee.onlineshop.ClassicDelivery">
        <constructor-arg value="999"/>
    </bean>

    <bean class="fr.uge.jee.onlineshop.OnlineShop" autowire="constructor">
        <constructor-arg value="AhMaZone"/>
    </bean>
</beans>